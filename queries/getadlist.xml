<query id="getadlist" action="select">
    <tables>
        <table name="plusad" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
	<conditions>
		<condition operation="more" column="enddate" var="nowdate" />
	</conditions>
	<navigation>
        <index var="sort_index" default="ad_srl" order="desc" />
        <list_count var="list_count" default="10" />
        <page_count var="page_count" default="10" />
        <page var="page" default="1" />
    </navigation>
</query>