<query id="getadlistuser" action="select">
    <tables>
        <table name="plusad" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
	<conditions>
        <condition operation="equal" column="member_srl" var="member_srl" notnull="notnull" />
    </conditions>
	<navigation>
        <index var="sort_index" default="ad_srl" order="desc" />
        <list_count var="list_count" default="10" />
        <page_count var="page_count" default="10" />
        <page var="page" default="1" />
    </navigation>
</query>